{
  "preset": "laravel",
  "rules": {
    "simplified_null_return": true,
    "align_multiline_comment": true,
    "array_indentation": true,
    "new_with_parentheses": {
      "anonymous_class": true,
      "named_class": true
    }
  },
  "exclude": [
    "vendor",
    "node_modules",
    "storage"
  ]
}
